<div class="row">
  <div class="large-3 medium-4 small-12 columns">
    <span class="label"><%= time %></span>
  </div>

  <div class="large-9 medium-8 small-12 columns">
    <h4 class="subheader"><%= title %></h4>
    <h5 class="subheader"><%= company %></h5>

    <ul class="square">
    <% duties.each do |d| %>
      <li><%= d %></li>
      <% end %>
    </ul>
  </div>
</div>